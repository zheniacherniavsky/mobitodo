{
  "name": "rnTodo",
  "version": "0.0.2-0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.{js,jsx}\"",
    "lint:fix": "eslint . --ext .js --fix",
    "np": "np --no-publish",
    "postversion": "react-native-version",
    "prepare": "husky install",
    "extract-msgs": "formatjs extract 'src/**/*.{js,jsx,ts,tsx}' --out-file ./locales/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile-msgs": "formatjs compile-folder ./locales ./src/assets/lang "
  },
  "dependencies": {
    "babel-plugin-formatjs": "^10.3.35",
    "expo-localization": "^14.0.0",
    "react": "18.1.0",
    "react-intl": "^6.2.5",
    "react-native": "0.70.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@formatjs/cli": "^5.1.12",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react-intl": "^3.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-formatjs": "^4.3.9",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "np": "^7.6.3",
    "prettier": "^2.8.1",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.1.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zheniacherniavsky/mobitodo"
  }
}
